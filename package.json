  "devDependencies": {
    "awesome-typescript-loader": "^3.2.3",
    "path": "^0.12.7",
    "shx": "^0.2.2",
    "typescript": "^2.5.3",
    "watchman": "^0.1.8",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@types/prop-types": "^15.5.2",
